{
  "pipeline": {
    "bootstrap:dev": {
      "outputs": ["cdk-outputs.json"]
    },
    "test": {
      "dependsOn": ["^bootstrap:dev"],
      "outputs": []
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "build:lambda": {
      "dependsOn": ["^build"],
      "outputs": ["target/"]
    },
    "synth": {
      "dependsOn": ["^build:lambda"],
      "outputs": ["cdk.out"]
    },
    "deploy": {
      "dependsOn": ["^build:lambda"],
      "outputs": ["cdk.out"]
    }
  }
}
